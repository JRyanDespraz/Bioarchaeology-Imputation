#Random Forest imputation
library(missForest)

set.seed(99)
RF_imputed_dataset <- missForest(as.matrix(dataset), maxiter=, ntree=, verbose=TRUE)
#Ryan-Despraz and Wissler (2024) set ntree equal to the number of variables
#Ryan-Despraz and Wissler (2024) set maxiter (maximum number of iterations) to 10, but it may stop sooner if the stopping criterion is met

#Create a dataframe for analyses
final_rf_dataframe <- as.data.frame(RF_imputed_dataset$ximp)


# Calculations used for comparing an imputed dataset to an original dataset
library(missMethods)
# These calculations are only for categorical data, therefore these rows/columns need to be specified
categ.precision <- evaluate_imputed_values(imputed_dataset[], original[], criterion = "precision")
nr_equal <- evaluate_imputed_values(imputed_dataset[], original[], criterion = "nr_equal")
res.MAE <- evaluate_imputed_values(imputed_dataset[], original[], criterion = "MAE")

# These calculations are only for quantitative data, therefore these rows/columns need to be specified
NRMSE_col_mean_sq <- evaluate_imputed_values(imputed_dataset[], original[], criterion = "NRMSE_col_mean_sq")
NRMSE_tot_sd <- evaluate_imputed_values(imputed_dataset[], original[], criterion = "NRMSE_tot_sd") 
